<UserControl x:Class="JobDetailModule.GoogleMapSearchView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:jobDetailModule="clr-namespace:JobDetailModule"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="500">
    <Grid>
        <WebBrowser jobDetailModule:WebBrowserProperties.Url="{Binding GoogleMapUrl}"/>
    </Grid>
</UserControl>
